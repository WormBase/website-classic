<VirtualHost *:8080>
   ServerName legacy.wormbase.org
#  PerlOptions +Parent
  PerlSwitches -Mlib=/usr/local/wormbase/website/legacy/extlib/lib/perl5/x86_64-linux-gnu-thread-multi \
               -Mlib=/usr/local/wormbase/website/legacy/extlib/extlib/lib/perl5 \
	       -Mlib=/usr/local/wormbase/website/legacy/gbrowse2/lib \
	       -Mlib=/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio \
	       -Mlib=/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics \
	       -Mlib=/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics/Glyph

   Include /usr/local/wormbase/website/legacy/conf/httpd.conf

   ScriptAlias  "/gb2"     "/usr/local/wormbase/website/legacy/gbrowse2/cgi/gb2"
   Alias       "/gb2-support/"     "/usr/local/wormbase/website/legacy/gbrowse2/"
   Alias       "/gbrowse2/i" "/usr/local/wormbase/website/legacy/tmp/gbrowse2/images"
   Alias       "/gbrowse2"   "/usr/local/wormbase/website/legacy/gbrowse2/html"


   <Directory "/usr/local/wormbase/website/legacy/gbrowse2/cgi/gb2">
       SetEnv PERL5LIB "/usr/local/wormbase/website/legacy/extlib/lib/perl5:/usr/local/wormbase/website/legacy/extlib/lib/perl5/x86_64-linux-gnu-thread-multi:/usr/local/wormbase/website/legacy/gbrowse2/lib:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics/Glyph"
       SetEnv GBROWSE_CONF   "/usr/local/wormbase/website/legacy/gbrowse2/conf"
    </Directory>

    <IfModule mod_fcgid.c>
    Alias /db/gb2 "/usr/local/wormbase/website/legacy/gbrowse2/cgi/gb2"
    <Location /db/gb2>
            SetHandler   fcgid-script
            Options       ExecCGI
            Order allow,deny
            Allow from all
        </Location>
        IdleTimeout 300
        FcgidInitialEnv GBROWSE_CONF /usr/local/wormbase/website/legacy/gbrowse2/conf
        FcgidInitialEnv DOCUMENT_ROOT /usr/local/wormbase/tmp/gbrowse/synteny
	FcgidInitialEnv PERL5LIB "/usr/local/wormbase/website/legacy/extlib/lib/perl5:/usr/local/wormbase/website/legacy/extlib/perl5/x86_64-linux-gnu-thread-multi:/usr/local/wormbase/website/legacy/gbrowse2/lib:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics:/usr/local/wormbase/website/legacy/gbrowse2/lib/Bio/Graphics/Glyph"
    </IfModule>


    Redirect /db/gb2 http://legacy.wormbase.org/gb2

</VirtualHost>
